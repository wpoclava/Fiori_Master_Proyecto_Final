<mvc:View controllerName="logaligroup.ProyectoFinal.HTML5Module.controller.Menu" xmlns:mvc="sap.ui.core.mvc" displayBlock="true" xmlns="sap.m">
    <Page id="page" title="{i18n>menuMain}">
        <content>
            <Panel headerText="{i18n>employees}" expandable="true" expanded="true">
                <GenericTile headerImage="sap-icon://add-employee" class="sapUiTinyMarginBegin sapUiTinyMarginTop" press="navToCreateEmployee">
                    <TileContent footer="{i18n>createEmployee}">
                    </TileContent>
                </GenericTile>
                <GenericTile headerImage="sap-icon://employee-lookup" class="sapUiTinyMarginBegin sapUiTinyMarginTop" press="navToShowEmployee">
                    <TileContent footer="{i18n>showEmployees}">
                    </TileContent>
                </GenericTile>
            </Panel>
            <Panel headerText="{i18n>orders}"  expandable="true" expanded="true">
                <GenericTile id="urlEmployees"  headerImage="sap-icon://signature" class="sapUiTinyMarginBegin sapUiTinyMarginTop" url="https://09d5d757trial-dev-logali-approuter.cfapps.us10.hana.ondemand.com/logaligroupEmployees/index.html">
                    <TileContent footer="{i18n>signOrder}">
                    </TileContent>
                </GenericTile>
            </Panel>
        </content>
    </Page>
</mvc:View>